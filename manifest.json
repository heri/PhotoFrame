{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "PhotoFrame",
  "version": "0.1",
  "manifest_version": 1,
  "description": "Chrome extension to view your pictures in full screen.",
  "offline_enabled": true,
  "icons": { "16": "icon_photoframe-16.png", "48": "icon_photoframe-48.png" },
  "background": {
    "scripts": ["gal_background.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "options_page": "gal_options.html",
  "browser_action": {
    "default_title": "PhotoFrame",
    "default_icon": "icon_photoframe-16.png"
  },
  "permissions": [
    "tabs"
  ]
}
